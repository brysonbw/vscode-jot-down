{
  "name": "jot-down",
  "displayName": "Jot Down",
  "description": "Quick and simple note taking-tool for vs code",
  "license": "MIT",
  "publisher": "brysonbw",
  "icon": "media/icon/jot-down.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/brysonbw/vscode-jot-down.git"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "note",
    "notes",
    "note taking",
    "productivity",
    "jot down",
    "todo",
    "todos",
    "task"
  ],
  "activationEvents": [
    "onView:jot-down-sidebar",
    "onCommand:jot-down.signIn",
    "onCommand:jot-down.refresh"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jot-down-sidebar-view",
          "title": "Jot Down",
          "icon": "media/edit.svg"
        }
      ]
    },
    "views": {
      "jot-down-sidebar-view": [
        {
          "type": "webview",
          "id": "jot-down-sidebar",
          "name": "Jot Down",
          "icon": "media/edit.svg",
          "contextualTitle": "Jot Down"
        }
      ]
    },
    "commands": [
      {
        "command": "jot-down.addNote",
        "category": "Jot Down",
        "title": "Add Note From Selection"
      },
      {
        "command": "jot-down.refresh",
        "category": "Jot Down",
        "title": "Refresh Sidebar Webview"
      },
      {
        "command": "jot-down.signIn",
        "category": "Jot Down",
        "title": "Sign In With GitHub"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "rollup -c && webpack --mode production",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "concurrently": "^7.2.2",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "rollup": "^2.76.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "polka": "^0.5.2"
  }
}
